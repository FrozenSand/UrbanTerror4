	////////////////////////////

	menuDef {

		name "psetup_menu"
		visible 0
		fullscreen 0
		rect 0 70 490 340
		focusColor 1 1 1 1
		style 1
		border 1
		onEsc   { close psetup_menu ; close setup_menu ; open main ; uiScript update "ui_SetName" }
		onOpen  { uiScript update "ui_GetName" ; setfocus window }
		onClose { uiScript update "ui_SetName" }


		itemDef {
			name window
			group grpPsetupbutton
			rect 10 10 436 321
			style 1
			border 1
			bordercolor .5 .5 .5 1
			forecolor 1 1 1 1
			backcolor 0 0 0 .8
			visible 1
			decoration
			mouseEnter { show psetupangle_on ; hide psetupangle }
			mouseExit { show psetupangle ; hide psetupangle_on }
		}

		itemDef {
			group grpPsetupbutton
			name psetupangle
			style WINDOW_STYLE_SHADER
			background "ui/assets/angle.tga"
			rect 430 12 16 16
			visible 1
			decoration
		}

		itemDef {
			group grpPsetupbutton
			name psetupangle_on
			style WINDOW_STYLE_SHADER
			background "ui/assets/angle_on.tga"
			rect 430 12 16 16
			visible 0
			decoration
		}

		itemDef {
			name areaname
			group grpControlbutton
			text "player setup"
			textscale .6
			textalignx 10
			textaligny 30
			rect 10 293 150 50
			style 1
			forecolor 1 1 1 .15
			visible 1
			decoration
		}


